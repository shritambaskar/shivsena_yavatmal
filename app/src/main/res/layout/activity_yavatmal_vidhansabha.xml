            android:layout_marginEnd="24dp"
            android:background="@drawable/rounded_bt"
            android:text="उपतालुका संघटीका"
            android:textColor="@color/white"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="@+id/uptaluka_pramukh"
